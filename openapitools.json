{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "storageDir": "../banca-fe/local-packages/banca-api",
    "generators": {
      "angular": {
        "generatorName": "typescript-angular",
        "inputSpec": "http://localhost:3000/api-json",
        "output": "../banca-fe/local-packages/banca-api",
        "glob": "",
        "additionalProperties": {
          "ngVersion": "11.2.5",
          "npmName": "restClient",
          "npmVersion": "6.9.0",
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
