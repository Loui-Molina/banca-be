    "express": "^4.17.1",
    "fastify-swagger": "^3.5.0",
    "jwt-decode": "^3.1.2",
    "latest-version": "^5.1.0",
    "mongoose": "^5.11.12",
    "nest-winston": "^1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
