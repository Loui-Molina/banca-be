  "scripts": {
    "api": " rimraf ../banca-fe/local-packages && openapi-generator-cli generate && node package.js",
    "api-validate": "openapi-generator-cli validate -i http://localhost:3000/api-json ",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "fixMeAndStart": "npm run prebuild && npm cache clean --force && npm install && npm run start:dev"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.4",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "^0.3.0",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.4",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/swagger": "^4.7.8",
    "@nestjs/terminus": "^7.0.1",
    "@openapitools/openapi-generator-cli": "^2.1.19",
    "@types/bcrypt": "^3.0.0",
